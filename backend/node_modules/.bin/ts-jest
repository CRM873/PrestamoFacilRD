#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/backend/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_ca598f06f58c5924542ad01b3511d195/node_modules/ts-jest/node_modules:/workspace/backend/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_ca598f06f58c5924542ad01b3511d195/node_modules:/workspace/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/backend/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_ca598f06f58c5924542ad01b3511d195/node_modules/ts-jest/node_modules:/workspace/backend/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_ca598f06f58c5924542ad01b3511d195/node_modules:/workspace/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_ca598f06f58c5924542ad01b3511d195/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_ca598f06f58c5924542ad01b3511d195/node_modules/ts-jest/cli.js" "$@"
fi
